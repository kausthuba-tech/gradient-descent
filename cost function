import numpy as np
def grad_desc(x,y):
    m_curr=b_curr=0
    iteration=1000
    n=len(x)
  for i in range(iteration);
      y_pred=m_curr*x+b_curr
      cost=(1/n)*sum([val**2 for val in (y-y_pred)])
      md=(2/n)*sum(x*(y-y_pred))
      bd=(2/n)*sum(y-y_pred)
      print("m {} b{} cost {} iteration {}".format(m,b,cost,i))
x=[3,6,9,12,14,8]
y=[12,25,36,18,7,4]
grad_desc(x,y)
      
